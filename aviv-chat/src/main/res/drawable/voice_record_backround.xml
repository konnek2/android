<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <size
                android:width="0dp"
                android:height="0dp"/>
            <solid
                android:color="@color/white"/>
            <stroke
                android:width="1dp"
                android:color="@color/white"/>
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp"
                />
        </shape>
    </item>

</selector>